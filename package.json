{
  "name": "ride-hailing",
  "private": true,
  "version": "1.0.0",
  "description": "Real-time ride-hailing system with Customer App, Partner App, and Admin Dashboard",
  "scripts": {
    "dev:backend": "pnpm --filter backend dev",
    "dev:customer": "pnpm --filter customer-app dev",
    "dev:partner": "pnpm --filter partner-app dev",
    "dev:admin": "pnpm --filter admin-dashboard dev",
    "dev": "pnpm run --parallel dev:backend dev:customer dev:partner dev:admin",
    "build": "pnpm run -r build",
    "lint": "pnpm run -r lint",
    "typecheck": "pnpm run -r typecheck"
  },
  "keywords": [
    "ride-hailing",
    "real-time",
    "websocket",
    "xstate"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "react-native-worklets": "^0.7.1"
  }
}
